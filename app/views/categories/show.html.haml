= render partial: 'top/header'
.category_wrapper
  .category_links
    - @category_links.each do |category|
      = link_to category.name, category_path(category)
.category_products
  %h2.category_products__category_name
    = "#{@category.name}の商品一覧"
  .category_product__box
    -if @category_products
      %ul.category_product_link
        = render partial: 'products/product', collection: @category_products, as: :product 
= render partial: 'top/footer'