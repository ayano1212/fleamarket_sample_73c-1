<h2>住所情報登録</h2>

<%= form_for @destination do |f| %>
  <%= render "devise/shared/error_messages", resource: @destination %>

  <div class="field">
    <%= f.label :destination_first_name %><br />
    <%= f.text_field :destination_first_name %>
  </div>

  <div class="field">
    <%= f.label :destination_family_name %><br />
    <%= f.text_field :destination_family_name %>
  </div>

    <div class="field">
    <%= f.label :destination_first_name_kana %><br />
    <%= f.text_field :destination_first_name_kana %>
  </div>

  <div class="field">
    <%= f.label :destination_family_name_kana %><br />
    <%= f.text_field :destination_family_name_kana %>
  </div>

    <div class="field">
    <%= f.label :postal_code %><br />
    <%= f.text_field :postal_code %>
  </div>

  <div class="field">
    <%= f.label :prefecture %><br />
    <%= f.text_field :prefecture %>
  </div>

  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>

  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </div>

  <div class="field">
    <%= f.label :building %><br />
    <%= f.text_field :building %>
  </div>

    <div class="field">
    <%= f.label :phone_number %><br />
    <%= f.text_field :phone_number %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>